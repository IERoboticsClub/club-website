---
interface Props {
    title: string;
}

const { title, } = Astro.props;
---

<p id="h" class="">{title}</p>
<style>
    #h {
        position: absolute;
        font-size: 30px;
        writing-mode: vertical-rl;
        text-orientation: mixed;
        transform: scale(-1, -1);
        top: 0;
        left: 200px;
        align-self: center;
    }
</style>
